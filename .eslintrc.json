{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-unused-vars": "off",
		"indent": ["warn", "tab"],
		"quotes": ["error", "single"]
	},
	"ignorePatterns": ["*.spec.js", "*.test.js"]
}
